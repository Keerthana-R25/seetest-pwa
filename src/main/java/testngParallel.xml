<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="PWAExample" configfailurepolicy="continue"  parallel="tests" thread-count="9" verbose="2">
   <test name="PWAAndroidDeviceChromeTest">
      <parameter name = "os" value="android"/>
      <parameter name = "browser" value="chrome"/>
      <!--<parameter name = "deviceId" value="d0429a1b7d24"/>-->
       <parameter name = "deviceId" value=""/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserDeviceTest"/>
      </classes>
   </test>
   <test name="IOSSafariDeviceTest">
      <parameter name = "os" value="ios"/>
      <parameter name = "browser" value="safari"/>
      <parameter name = "deviceId" value=""/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserDeviceTest"/>
      </classes>
   </test>
   <test name="IOSSafariTabletTest">
      <parameter name = "os" value="ios"/>
      <parameter name = "browser" value="safari"/>
      <parameter name = "deviceId" value=""/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserDeviceTest"/>
      </classes>
   </test>
   <test name="ChromeWebTest">
      <parameter name = "browser" value="chrome"/>
      <parameter name = "os" value="windows"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>
   <test name="MACSafariWebTest">
      <parameter name = "browser" value="safari"/>
      <parameter name = "os" value="Mac"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>
   <test name="EdgeWinWebTest">
      <parameter name = "browser" value="internet explorer"/>
      <parameter name = "os" value="windows"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>
   <test name="ChromeMacWebTest">
      <parameter name = "browser" value="chrome"/>
      <parameter name = "os" value="Mac"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>
   <test name="FirefoxWinWebTest">
      <parameter name = "browser" value="firefox"/>
      <parameter name = "os" value="windows"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>
   <test name="FirefoxMacWebTest">
      <parameter name = "browser" value="firefox"/>
      <parameter name = "os" value="mac"/>
      <classes>
         <class name="io.seetest.pwa.tests.CrossBrowserWebTest"/>
      </classes>
   </test>

</suite>